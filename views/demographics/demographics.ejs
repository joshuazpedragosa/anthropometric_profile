<!-- cards -->
<div class="flex flex-wrap lg:flex-nowrap gap-4 pt-24 px-8">

    <div class="w-screen">
        <div class="bg-white h-auto p-4 rounded-lg shadow-xl">
            <div class="text-xl text-gray-700">
                <i class='bx bx-file-find text-green-500' ></i>
                Search/Filters
            </div>
            <div class="flex flex-wrap gap-4">
                <div>
    
                    <div class="flex flex-col my-4">
                        <label for="#searchQCode">Search by QCode/Fullname:</label>
                        <input type="text" onkeyup="searchQCodeName()" id="searchQCode" class="border outline-none px-2 text-gray-600 focus:border-green-500">
                    </div>
    
                    <div class="flex flex-col my-4">
                        <label for="#filterAge">Filter By Age:</label>
                        <input type="number" onkeyup="searchAge()" id="filterAge" class="border outline-none px-2 text-gray-600 focus:border-green-500">
                    </div>
    
                    <div class="flex flex-col my-4">
                        <label for="">Filter By Municipality:</label>
                        <input type="text" onkeyup="searchMunicipality()" id="filterMunicipality" class="border outline-none px-2 text-gray-600 focus:border-green-500">
                    </div>
    
                </div>
    
                <div>
    
                    <div class="flex flex-col my-4">
                        <label for="#filterAge">Filter By Baranggay:</label>
                        <input type="text" onkeyup="searchBrgy()" id="filterbrgy" class="border outline-none px-2 text-gray-600 focus:border-green-500">
                    </div>
    
                    <div class="flex flex-col my-4">
                        <label for="">Filter By Province:</label>
                        <input type="text" onkeyup="searchProvince()" id="filterProvince" class="border outline-none px-2 text-gray-600 focus:border-green-500">
                    </div>
                </div>
    
                <div>
                    <div class="flex flex-col my-4">
                        <label for="">Search by Farm Involvement:</label>
                        <input type="text" onkeyup="searchInvolvement()" id="Involvement" class="border outline-none px-2 text-gray-600 focus:border-green-500">
                    </div>
    
                </div>
            </div>
        </div>
    </div>
    <div class="w-screen">
        <div class="bg-white p-2 shadow-xl rounded-xl">
            <div class="p-2 text-lg">
                <i class='bx bx-calculator text-green-500'></i>
                Statistics
            </div>
    
            <div class="mx-2">
                <div class="flex my-1 border-b">
                    <div>MEAN</div>
                    <div class="ms-auto" id="mean"></div>
                </div>
    
                <div class="flex my-1 border-b">
                    <div>STANDARD DEVIATION</div>
                    <div class="ms-auto" id="standard_dev"></div>
                </div>
    
                <div class="flex my-1 border-b">
                    <div>RANGE MIN</div>
                    <div class="ms-auto" id="rangeMin"></div>
                </div>
    
                <div class="flex my-1 border-b">
                    <div>RANGE MAX</div>
                    <div class="ms-auto" id="rangeMax"></div>
                </div>
                
                <div class="flex my-1 border-b">
                    <div>CV(%)</div>
                    <div class="ms-auto" id="cvPercent"></div>
                </div>
    
                <div class="flex my-1 border-b">
                    <div>5th Percentile</div>
                    <div class="ms-auto" id="percentile5th"></div>
                </div>
    
                <div class="flex my-1 border-b">
                    <div>50th Percentile</div>
                    <div class="ms-auto" id="percentile50th"></div>
                </div>
    
                <div class="flex my-1 border-b">
                    <div>95th Percentile</div>
                    <div class="ms-auto" id="percentile95th"></div>
                </div>
            </div>
        </div>
    </div>

    <!-- 
 -->

</div>
<!-- end cards -->
<!-- table -->
<div class=" mt-4 max-w-screen">
    <div class=" p-2 bg-white rounded-xl shadow-xl mx-4">
        <div class="p-2 text-lg">
            <i class='bx bx-table text-green-500'></i>
            Demographics Table
        </div>
        <div class="max-h-96 max-w-auto overflow-y-auto overflow-x-auto">
            <table class="max-w-80">
                <thead>
                    <tr class="bg-green-400">
                        <th class="hidden px-8 py-2">Action</th>
                        <th class="px-8 py-2">QCode</th>
                        <th class="px-8 py-2">Surname</th>
                        <th class="px-8 py-2">Given Name</th>
                        <th class="px-8 py-2">Middle Name</th>
                        <th class="px-8 py-2">Date Measured</th>
                        <th class="px-8 py-2">Age</th>
                        <th class="px-8 py-2">Sex</th>
                        <th class="px-8 py-2">Contact No.</th>
                        <th class="px-8 py-2">Position</th>
                        <th class="px-8 py-2">Marital Status</th>
                        <th class="px-8 py-2">PWD</th>
                        <th class="px-8 py-2">Barangay</th>
                        <th class="px-8 py-2">Municipality</th>
                        <th class="px-8 py-2">Province</th>
                        <th class="px-8 py-2">Region</th>
                        <th class="px-8 py-2">Highest Educational Attainment</th>
                        <th class="px-8 py-2">Organization's Name</th>
                        <th class="px-8 py-2">Farm Commodity</th>
                        <th class="px-8 py-2">Farming Activities</th>
                        <th class="px-8 py-2">Farm Involvement</th>
                        <th class="px-8 py-2">Proprietorship</th>
                        <th class="px-8 py-2">Farm Area</th>
                        <th class="px-8 py-2">Types of Machinery</th>
                        <th class="px-8 py-2">Years on Farming Experience</th>
                        <th class="px-8 py-2">Farming Occupation Status</th>
                    </tr>
                </thead>
                <tbody id="demographicsTbl">

                </tbody>
            </table>
        </div>
    </div>
</div>
<!-- end table -->